{
  "name": "RTL_433 to MQTT Bridge",
  "version": "0.1",
  "slug": "rtl4332mqtt",
  "description": "433MHz RF to MQTT Bridge based on RTL_SDR/RTL_433 for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/gschindel/hassio-addons/tree/master/rtl4332mqtt",
  "startup": "before",
  "boot": "auto",
  "map": ["config:rw", "ssl"],
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "host_network": "False",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "options":
  {
    "mqtt_host": "hassio.local",
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "rtl433[/model]",
    "protocol": "-R 71 -R 12",
    "frequency": "",
    "gain": "",
    "frequency_offset": ""
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "protocol": "str",
    "frequency": "str",
    "gain": "str",
    "frequency_offset": "str"
   }
}
